@startuml
skinparam monochrome true
skinparam classAttributeIconSize 0
scale 2

class ClassInfo {
  +classname
  +bases
  +classvars
  +members
  +methods
  -__init__()
  +visibility()
  +add_classvar()
  +add_member()
  +add_method()
  +done()
}

class TreeVisitor {
  +context
  +classinfo
  +constructor
  -__init__()
  +visit_ClassDef()
  +visit_FunctionDef()
  +visit_Assign()
}

class PUML_Generator {
  +dest
  +tabs
  +namespaces
  +namespaces
  -__init__()
  +depth()
  +indent()
  +header()
  +footer()
  +print_classinfo()
}

@enduml
